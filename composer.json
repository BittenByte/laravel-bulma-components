{
    "name": "bittenbyte/laravel-bulma-components",
    "type": "library",
    "description": "View components and Sublime Text snippets to integrate Bulma.io easier into your Laravel 5.4 (or more) projects",
    "keywords": [
        "laravel",
        "email",
        "forward",
        "post"
    ],
    "homepage": "https://github.com/BittenByte/laravel-bulma-components",
    "license": "MIT",
    "authors": [
        {
            "name": "Jose H. MilÃ¡n",
            "email": "jose@bittenbyte.io",
            "homepage": "http://www.bittenbyte.io",
            "role": "CEO, Co-Founder"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "laravel/framework": ">=5.4.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1",
        "squizlabs/php_codesniffer": "~2.3"
    },
    "autoload": {
        "psr-4": {
            "BittenByte\\LaravelBulmaComponents\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BittenByte\\LaravelBulmaComponents\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
